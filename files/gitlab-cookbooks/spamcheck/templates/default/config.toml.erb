[grpc]
port = "<%= @port %>"

[rest]
externalPort = "<%= @external_port %>"

[logger]
level = "<%= @log_level %>"
format = "<%= @log_format %>"
output = "<%= @log_output %>"

[monitor]
address = "<%= @monitoring_address %>"

[extraAttributes]
monitorMode = "<%= @monitor_mode %>"

[filter]
[filter.allowList]
<% @allowlist.each do |key, value| %>
<%= key %> = "<%= value %>"
<% end %>
[filter.denyList]
<% @denylist.each do |key, value| %>
<%= key %> = "<%= value %>"
<% end %>

[preprocessor]
socketPath = "<%= @preprocessor_socket_path %>"

[modelAttributes]
modelPath = "<%= @preprocessor_model_path %>"
